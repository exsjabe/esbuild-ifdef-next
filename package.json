{
  "name": "esbuild-ifdef",
  "version": "0.2.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "",
  "scripts": {
    "build": "tsup index.ts --dts --format cjs,esm",
    "format": "prettier --write . --cache --log-level warn"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/exsjabe/esbuild-ifdef-next.git"
  },
  "peerDependencies": {
    "esbuild": "^0.15.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
